    "import math\n",
    "print(2**11-((math.factorial(11))/(math.factorial(4)*math.factorial(7))+(math.factorial(11))/(math.factorial(9)*math.factorial(2))))\n",
    "\n",
    "count = 0\n",
    "for x in range(2**11):\n",
    "    s = bin(x)[2:]\n",
    "    if (8 + s.count('1'))%5 != 0:\n",
    "        count += 1\n",
    "print(count)"
